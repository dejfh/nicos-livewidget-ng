#
# Some compiler configuration, escpecially c++11 support.
#

greaterThan(QT_MAJOR_VERSION, 4) {
	CONFIG += c++11
} else {
	!msvc: QMAKE_CXXFLAGS += -std=c++11
}

msvc {
	QMAKE_CXXFLAGS += -D _CRT_SECURE_NO_WARNINGS
} else {
	QMAKE_CXXFLAGS +=-Wall -Wextra -pedantic
}

INCLUDEPATH += $$PWD

QMAKEFEATURES += C:/Dev/projects/Cpp/nicos-livewidget-ng/
